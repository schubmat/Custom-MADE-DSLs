GenericRelationship:
	'Generic Relationship'
	name?=STRING
	targetElement=(DRObjects)
	sourceElement=(DRObjects)
	sourceCardinality=INT
	targetCardinality=INT
	sourceRole?=STRING
	targetRole?=STRING;

	

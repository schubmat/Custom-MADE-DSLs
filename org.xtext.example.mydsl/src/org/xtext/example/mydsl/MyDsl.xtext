grammar org.xtext.example.mydsl.MyDsl with org.eclipse.xtext.common.Terminals

import "http://www.eclipse.org/emf/2002/Ecore" as ecore
generate myDsl "http://www.xtext.org/example/mydsl/MyDsl"

M1_Model: 
	models+=ModelRoot;
	
ModelRoot:
	'Language' 'Name:' name=ID '

Language' 'Types:
' '	'	elements+=(DRObjects)+ '

Associations:
' '	' 	associationElements+=(Relationship)+;

DRObjects:
	Statement | 
	Decision_Problem |
	Decision_Result |
	Decision_Option
;

Statement:
	'Statement' 'Type:
' '	ID:'	name=ID '
Type-Name:' 	text=STRING;
	
Decision_Problem:
	'Decision' 'Problem' 'Type:
' '	ID:'	name=ID '
Type-Name:' text=STRING;

Decision_Result:
	'Decision' 'Result' 'Type:
' '	ID:'	name=ID '
Type-Name:' result=(Decision_Option);

Decision_Option:
	'Decision' 'Option' 'Type:
' '	ID:'	name=ID '
Type-Name:' text=STRING;

Relationship:
	GenericRelationship | 
	Argumentative_Relationship |
	Derivative_Relationship | 
	Consequence_Relationship |
	Option_Relationship;
	
Argumentative_Relationship:
	source=(Statement)
	source_cardinality=STRING
	target=DRObjects
	target_cardinality=STRING;

Derivative_Relationship:
	source=(Decision_Problem)
	source_cardinality=STRING
	target=(Decision_Problem | Decision_Result)
	target_cardinality=STRING;

Consequence_Relationship:
	source=(Decision_Option)
	source_cardinality=STRING
	target=(Decision_Problem | Decision_Result)
	target_cardinality=STRING;
	
Option_Relationship:
	source=(Decision_Problem)
	source_cardinality=STRING
	target=(Decision_Option)
	target_cardinality=STRING;
	
